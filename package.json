{
  "name": "with-tailwind",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start-user-app": "cd ./apps/web && npm run start"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "ts-jest": "^29.4.0",
    "turbo": "^2.5.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.44",
    "chart.js": "^4.5.0",
    "recharts": "^2.15.3"
  }
}
